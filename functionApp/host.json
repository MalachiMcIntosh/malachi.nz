{
  "version": "2.0",
  "extensions": {
        "http": {
            "maxOutstandingRequests": 2,
            "maxConcurrentRequests": 1,
            "dynamicThrottlesEnabled": true
        }
    }
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[1.*, 2.0.0)"
  }, 
  "logging": {
    "fileLoggingMode": "always",
    "logLevel": {
      "Host.Triggers.CosmosDB": "Trace"
    }
  }
}
